				{
					"listen": "test",
					"script": {
						"exec": [
							"\r",
							"var jsonData = pm.response.json();\r",
							"var respToken = jsonData.token;\r",
							"pm.environment.set(\"token\", respToken) // стандартный сниппет для сохранения токена"
						],
						"type": "text/javascript"
					}
				}
			],
